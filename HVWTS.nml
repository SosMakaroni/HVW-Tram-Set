    }
    graphics {
		default:                 		sw_FA;
		purchase:						swg_FA_purchase;
		articulated_part:        		sw_FA_articulated;
		length:                  		sw_FA_length;
		cost_factor:					8 * parapuco;
		running_cost_factor:			6 * pararuco;
		additional_text: 				string(STR_FA_ADAT);
