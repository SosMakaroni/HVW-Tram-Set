    0: item_COMbp;
    1: item_COMbp_2;
    2: item_COMbp_3;
    3: item_COMbp_4;
    4: item_COMbp_5;
    5: item_COMbp_6;
    CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}
