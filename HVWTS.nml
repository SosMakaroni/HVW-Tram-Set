switch(FEAT_ROADVEHS, SELF, sw_COMbp_namep, (extra_callback_info1 >> 8) & 0xFFFF) {
    1: return string(STR_COMbp_BUY);
	return CB_RESULT_NO_TEXT;
}
switch(FEAT_ROADVEHS, SELF, sw_COMbp_name, extra_callback_info1 & 0xFF) {
    0x11: return string(STR_COMbp_INFO); // Jármű infóban név
    0x20: sw_COMbp_namep; // Vásárlási lista név
	0x21: return string(STR_COMbp_NAME); // Elővásárlási név
	return CB_RESULT_NO_TEXT;
}


