   0x10: sw1_BE1ai; // Depó nézet
   0x11: sw1_BE1ai; // Jármű infó/Refit nézet
   0x12: sw1_BE1ai; // Jármű lista nézet
   0x00: sw1_BE1a; // Játék nézet
}

