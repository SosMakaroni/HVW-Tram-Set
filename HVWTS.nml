    0: item_BE1a;
    1: item_BE1a_2;
    2: item_BE1a_3;
    3: item_BE1a_T;
    CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

// Modelhossz
switch (FEAT_ROADVEHS, SELF, sw_BE1a_length, position_in_consist) {
	1: return 4;
	2: return 3;
	3: return 8;
    return 8;
}

// Szövegek
switch(FEAT_ROADVEHS, SELF, sw_BE1a_namep, (extra_callback_info1 >> 8) & 0xFFFF) {
    1: return string(STR_BE1_BUY);
    2: return string(STR_BE1a_BUY);
	return CB_RESULT_NO_TEXT;
}
