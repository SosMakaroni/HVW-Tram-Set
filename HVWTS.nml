    0: item_CAFD;
    1: item_CAFD_2;
    2: item_CAFD_3;
    3: item_CAFD_4;
    4: item_CAFD_5;
    5: item_CAFD_T;
    CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

