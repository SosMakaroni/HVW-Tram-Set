// Vásárlási grafika
switch (FEAT_ROADVEHS, SELF, swg_CAFBPH_purchase, [
    STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100),
    getbits(extra_callback_info1, 8, 8)
    ]) {
    0: return ss_CAFBPH_purchase;    // Alap modell
    1: return ss_CAFBPH_P_purchase;    // Pantonograf (le)
}

// Lista grafika
switch (FEAT_ROADVEHS, SELF, sgw_CAFBPH_3i, [
    STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100),
    getbits(extra_callback_info1, 8, 8)
    ]) {
    0: return ss_CAFBPH_3i;    // Alap modell
    1: return ss_CAFBPH_3_PUPi;    // Pantonograf (fel)
}
switch (FEAT_ROADVEHS, SELF, sgw_CAFBPH_7i, [
    STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100),
    getbits(extra_callback_info1, 8, 8)
    ]) {
    0: return ss_CAFBPH_7i;    // Alap modell
    1: return ss_CAFBPH_7_PUPi;    // Pantonograf (fel)
}
switch (FEAT_ROADVEHS, SELF, swl_CAFBPHi, position_in_consist ) {
    1: ss_CAFBPH_2i;
	2: sgw_CAFBPH_3i;
	3: ss_CAFBPH_4i;
	4: ss_CAFBPH_5i;
	5: ss_CAFBPH_6i;
	6: sgw_CAFBPH_7i;
	7: ss_CAFBPH_8i;
	8: ss_CAFBPH_9i;
	9: ss_toldat;
    ss_CAFBPH_1i;
}

// Játék grafika
switch (FEAT_ROADVEHS, SELF, sgw_CAFBPH_3, [
    STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100),
