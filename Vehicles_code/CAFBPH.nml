}
switch (FEAT_ROADVEHS, SELF, sgw_CAFBPH_7, [
    STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100),
    getbits(extra_callback_info1, 8, 8)
    ]) {
    0: return sg_CAFBPH_7;    // Alap modell
    1: return ss_CAFBPH_7_PUP;    // Pantonograf (fel)
}
switch (FEAT_ROADVEHS, SELF, swl_CAFBPH, position_in_consist ) {
    1: sg_CAFBPH_2;
	2: sgw_CAFBPH_3;
	3: sg_CAFBPH_4;
	4: sg_CAFBPH_5;
	5: sg_CAFBPH_6;
	6: sgw_CAFBPH_7;
	7: sg_CAFBPH_8;
	8: sg_CAFBPH_9;
	9: ss_toldat;
    sg_CAFBPH_1;
}

// Rajz
switch(FEAT_ROADVEHS, SELF, sw_CAFBPH, (extra_callback_info1 & 0x12)) {
   0x10: swl_CAFBPHi; // Depot GUI
   0x11: swl_CAFBPHi; // Vehicle Details/Refit GUI
   0x12: swl_CAFBPHi; // Vehicle List
   0x00: swl_CAFBPH; // Vehicle in game
}

// Csuklosítás
switch (FEAT_ROADVEHS, SELF, sw_CAFBPH_articulated, extra_callback_info1) {
    0: item_CAFBPH;
    1: item_CAFBPH_2;
    2: item_CAFBPH_3;
    3: item_CAFBPH_4;
    4: item_CAFBPH_5;
    5: item_CAFBPH_6;
    6: item_CAFBPH_7;
    7: item_CAFBPH_8;
    8: item_CAFBPH_9;
    9: item_CAFBPH_T;
    CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

// Modelhossz
switch (FEAT_ROADVEHS, SELF, sw_CAFBPH_length, position_in_consist) {
	1: return 6;
	2: return 4;
	3: return 6;
	4: return 4;
	5: return 6;
	6: return 4;
	7: return 6;
	8: return 2;
	9: return 8;
    return 7;
}

// Szövegek
switch(FEAT_ROADVEHS, SELF, sw_CAFBPH_namep, (extra_callback_info1 >> 8) & 0xFFFF) {
    1: return string(STR_CAFBPH_BUY);
	return CB_RESULT_NO_TEXT;
}
switch(FEAT_ROADVEHS, SELF, sw_CAFBPH_name, extra_callback_info1 & 0xFF) {
    0x11: return string(STR_CAFBPH_INFO); // Jármű infóban név
    0x20: sw_CAFBPH_namep; // Vásárlási lista név
	0x21: return string(STR_CAFBPH_NAME); // Elővásárlási név
	return CB_RESULT_NO_TEXT;
}


item(FEAT_ROADVEHS, item_CAFBPH) {
	property {
        name:								string(STR_CAF_NAME);
        climates_available:				bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPICAL);
        introduction_date:				date(2016,03,29);
        model_life:						20;
        vehicle_life:						40;
        reliability_decay:				20;
        refittable_cargo_classes:		bitmask(CC_PASSENGERS);
        non_refittable_cargo_classes:	bitmask(CC_PIECE_GOODS, CC_EXPRESS, CC_REFRIGERATED);
        loading_speed:					12;
        sprite_id:							SPRITE_ID_NEW_ROADVEH;
        speed:								50 km/h;
        misc_flags:						bitmask(ROADVEH_FLAG_2CC, ROADVEH_FLAG_TRAM, ROADVEH_FLAG_SPRITE_STACK);
        refit_cost:						0;
        running_cost_base:				RUNNING_COST_ROADVEH;
        power:								840 kW;
        weight:							63 ton;
        cargo_capacity:					39;
		cargo_age_period:					185;
        sound_effect:						SOUND_POWER_STATION;
		variant_group:					item_CAFD;
    }
    graphics {
		default:                 		sw_CAFBPH;
		purchase:						swg_CAFBPH_purchase;
		articulated_part:        		sw_CAFBPH_articulated;
		length:                  		sw_CAFBPH_length;
		cost_factor:					90 * parapuco;
		running_cost_factor:			122 * pararuco;
		additional_text: 				string(STR_CAFBPH_ADAT);
		name:							sw_CAFBPH_name;
    }
}
